<p id="notice">
  <%= notice %>
</p>

<h1>Continents</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @continents.each do |continent| %>
    <tr>
      <td>
        <%= continent.name %>
      </td>


    </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Continent', new_continent_path %>