<p id="notice">
  <%= notice %>
</p>

<h1>Sources</h1>

<table>
  <thead>
    <tr>
      <th>Identity</th>
      <th>Name</th>
      <th>Category</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sources.each do |source| %>
    <tr>
      <td>
        <%= source.identity %>
      </td>
      <td>
        <%= source.name %>
      </td>
      <td>
        <%= source.category.name %>
      </td>

    </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Source', new_source_path %>