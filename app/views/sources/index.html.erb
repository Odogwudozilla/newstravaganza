<p id="notice">
  <%= notice %>
</p>

<h1>Sources</h1>

<table>
  <thead>
    <tr>
      <th>identitiy</th>
      <th>name</th>
      <th>category_id</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sources.each do |source| %>
    <tr>
      <td>
        <%= source.identitiy %>
      </td>
      <td>
        <%= source.name %>
      </td>
      <td>
        <%= source.category.name %>
      </td>
      <td>
        <%= link_to 'Show', source %>
      </td>
      <td>
        <%= link_to 'Edit', edit_source_path(source) %>
      </td>
      <td>
        <%= link_to 'Destroy', source, method: :delete, data: { confirm: 'Are you sure?' } %>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>

<br>